{
  "name": "@phantasm0009/lazy-import",
  "version": "1.0.0",
  "description": "Smart, dynamic imports that feel static. Improve your application's startup time and bundle size by loading modules only when needed.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build",
    "demo": "node serve-demo.js",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "import",
    "dynamic-import",
    "lazy-loading",
    "performance",
    "optimization",
    "tree-shaking",
    "lazy",
    "modules",
    "typescript",
    "nodejs",
    "browser",
    "cache",
    "preload",
    "phantasm0009"
  ],
  "author": "Phantasm0009 <atiwar0414@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "jest": "^27.4.7",
    "rollup": "^2.67.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phantasm0009/lazy-import.git"
  },
  "bugs": {
    "url": "https://github.com/Phantasm0009/lazy-import/issues"
  },
  "homepage": "https://github.com/Phantasm0009/lazy-import#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
