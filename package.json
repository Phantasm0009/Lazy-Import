{  "name": "@phantasm0009/lazy-import",
  "version": "1.1.0",
  "description": "Production-ready lazy loading with Static Bundle Helper (SBH). Smart caching, intelligent preloading, and seamless bundler integration. 19/19 tests passing, 4/4 bundlers supported.","main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./bundler": {
      "import": "./dist/bundler.esm.js",
      "require": "./dist/bundler.js",
      "types": "./dist/bundler.d.ts"
    },
    "./babel": {
      "import": "./dist/babel.esm.js",
      "require": "./dist/babel.js",
      "types": "./dist/babel.d.ts"
    },
    "./rollup": {
      "import": "./dist/rollup.esm.js",
      "require": "./dist/rollup.js",
      "types": "./dist/rollup.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.esm.js",
      "require": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    },
    "./webpack": {
      "import": "./dist/webpack.esm.js",
      "require": "./dist/webpack.js",
      "types": "./dist/webpack.d.ts"
    },
    "./esbuild": {
      "import": "./dist/esbuild.esm.js",
      "require": "./dist/esbuild.js",
      "types": "./dist/esbuild.d.ts"
    }
  },  "bin": {
    "lazy-import": "./dist/cli.js"
  },  "files": [
    "dist",
    "README.md",
    "TUTORIAL.md",
    "MIGRATION.md",
    "API.md",
    "CHANGELOG.md",
    "FAQ.md",
    "LEARN.md",
    "DOCS.md",
    "DEVELOPMENT.md",
    "CONTRIBUTING.md",
    "SECURITY.md",
    "examples",
    "LICENSE"
  ],"scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:integration": "node tests/integration-test.js",
    "test:performance": "node tests/performance-benchmark.js",
    "test:edge-cases": "node tests/edge-case-tests.js",
    "test:comprehensive": "node tests/comprehensive-test-suite.js",
    "test:all": "npm run test && npm run test:comprehensive",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"examples/**/*.{js,ts,tsx}\" \"*.md\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"examples/**/*.{js,ts,tsx}\" \"*.md\"",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "demo": "node serve-demo.js",
    "prepublishOnly": "npm run test && npm run build",
    "analyzer": "node dist/cli.js",
    "analyze": "npm run analyzer",
    "clean": "rimraf dist coverage .nyc_output",
    "prebuild": "npm run clean",
    "pretest": "npm run build",
    "release": "npm run test:all && npm run build && npm publish",
    "release:dry": "npm run test:all && npm run build && npm publish --dry-run",
    "docs:generate": "typedoc src/index.ts --out docs/api --readme README.md",
    "docs:serve": "http-server docs -p 8080",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm run test:all",
    "ci": "npm run validate && npm run build"
  },  "keywords": [
    "import",
    "dynamic-import",
    "lazy-loading",
    "performance",
    "optimization",
    "tree-shaking",
    "lazy",
    "modules",
    "typescript",
    "nodejs",
    "browser",
    "cache",
    "preload",
    "static-bundle-helper",
    "sbh",
    "bundler",
    "webpack",
    "vite",
    "rollup",
    "esbuild",
    "babel",
    "production-ready",
    "phantasm0009"
  ],
  "author": "Phantasm0009 <atiwar0414@gmail.com>",
  "license": "MIT",  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/types": "^7.20.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/babel__core": "^7.1.20",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.0.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "esbuild": "^0.17.0",
    "eslint": "^8.8.0",
    "http-server": "^14.1.1",
    "jest": "^27.4.7",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup": "^2.67.0",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.24.8",
    "typescript": "^4.5.5",
    "vite": "^4.0.0",
    "webpack": "^5.75.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phantasm0009/lazy-import.git"
  },
  "bugs": {
    "url": "https://github.com/Phantasm0009/lazy-import/issues"
  },
  "homepage": "https://github.com/Phantasm0009/lazy-import#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },  "dependencies": {}
}
